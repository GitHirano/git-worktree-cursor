{
  "name": "git-worktree-cursor",
  "displayName": "Git Worktree + Cursor Launcher",
  "description": "Create Git worktrees and open them in Cursor editor",
  "version": "0.0.1",
  "publisher": "your-publisher-name",
  "icon": "icon.svg",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/git-worktree-extension"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other",
    "SCM Providers"
  ],
  "keywords": [
    "git",
    "worktree",
    "cursor",
    "branch",
    "workspace"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "git-worktree-cursor.addWorktree",
        "title": "Git: Add Worktree and Open in Cursor"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "^16.18.126",
    "@types/vscode": "^1.100.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "typescript": "^4.9.3"
  }
}
